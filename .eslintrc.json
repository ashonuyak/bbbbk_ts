{
    "env": {
        "browser": true,
        "es2021": true,
				"node": true
    },
    "extends": [
			"airbnb"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
				"prettier"
    ],
    "rules": {
        "linebreak-style":"off",
				"semi": "off"
        // "object-curly-newline":"off",
        // "no-useless-return":"off"
    }
}
